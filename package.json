{
  "name": "multiplayer-server",
  "version": "1.0.0",
  "description": "A mulitplayer game server",
  "main": "index.js",
  "scripts": {
    "watch-node": "nodemon dist/app.js",
    "build": "npm run build-ts && npm run tslint && npm run build-ui",
    "build-ts": "tsc",
    "build-ui": "cd ui && npm run build",
    "watch-ts": "tsc -w",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold\" \"npm run watch-ts\" \"npm run watch-node\" ",
    "serve": "node dist/app.js",
    "start": "npm run serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "serve-debug": "nodemon --inspect dist/app.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold\" \"npm run watch-ts\" \"npm run serve-debug\" "
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.2",
    "log4js": "^2.3.11",
    "socket.io": "^2.0.4",
    "universal-cookie": "^2.1.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "nodemon": "^1.14.11",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2"
  }
}